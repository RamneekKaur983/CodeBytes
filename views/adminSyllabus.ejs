    <body>
    <div class="container">

   





<div class="collection " >

    <form action="/deleteTitle" method="POST">
        <% topicNames.forEach(function(item) { %>

            <p style="background-color: white;">
                <label>
                  <input id="indeterminate-checkbox" type="checkbox" name="TopicToDelete"  value="<%=item.title %>"/>
               
                  <span> <a href= "/adminSyllabus/<%= item .title %> " ><%= item.title %></a></span>
                </label>
              </p>
           
            <br>
    
    
            <% } )%>

         <div style="text-align: center;">
            <button class="btn waves-effect waves-light" type="submit" name="action" style="background-color: #665998; text-align: center;" >
                <i class="material-icons right">Delete Selected Topics</i>
              </button>
        
         </div>
              
    </form>
  
        
           

</div>
<br>
<div >
<form action="/adminSyllabus"  method="POST" style="background-color: white; text-align: center;">

