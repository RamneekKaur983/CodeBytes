<%- include('header'); -%>



<form action="/adminAdd" method="POST">
   
    <input type="text" name="firstName" id="firstName" placeholder="Enter New First Name">
    
    <input type="text" name="lastName" id="lastName" placeholder="Enter New Last Name">
    
    <input type="text" name="email" id="email" placeholder="Enter New Email">
    <label for="psw">Password</label>
    <input name="password" type="password" id="psw" name="psw" pattern="(?=.*\d)(?=.*[a-z])(?=.*[A-Z]).{8,}" title="Must contain at least one number and one uppercase and lowercase letter, and at least 8 or more characters" required>
    
    <button class="btn waves-effect waves-light" type="submit" name="action">Submit
        <i class="material-icons right">send</i>
      </button>
    
    
    </form>









<%- include('footer'); -%>